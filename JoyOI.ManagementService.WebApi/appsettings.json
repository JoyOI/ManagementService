{
	"ConnectionStrings": {
		"DefaultConnection": "Server=127.0.0.1;Port=3306;Database=joyoi;User Id=root;Password=123456;"
	},
	"Logging": {
		"IncludeScopes": false,
		"LogLevel": {
			"Default": "Debug",
			"System": "Information",
			"Microsoft": "Information"
		}
	},
	"JoyOIManagement": {
		"Name": "Default",
		"Container": {
			"MaxRunningJobs": 32,
			"WorkDir": "/workdir/",
			"ActorCodePath": "actor/Program.cs",
			"ActorExecuteCommand": "sh run-actor.sh"
		},
		"Limitation": {
			"CPUPeriod": 1000000,
			"CPUQuota": 1000000,
			"Memory": 268435456,
			"MemorySwap": 268435456,
			"StorageBaseSize": 3
		},
		"Nodes": {
			"docker-1": {
				"Image": "joyoi",
				"Address": "http://docker-1:2376",
				"ClientCertificatePath": "ClientCerts/docker-1.pfx",
				"ClientCertificatePassword": "123456"
			},
			"docker-2": {
				"Image": "joyoi",
				"Address": "http://docker-2:2376",
				"ClientCertificatePath": "ClientCerts/docker-2.pfx",
				"ClientCertificatePassword": "123456"
			}
		}
	}
}
